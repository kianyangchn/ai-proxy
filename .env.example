# JWT configuration for application-level authentication
AUTH_JWT_SECRET=change-me
AUTH_JWT_AUDIENCE=ai-proxy
AUTH_JWT_ISSUER=issuer-id
AUTH_JWT_ALGORITHM=HS256

# Rate limit settings (requests per minute per client id)
RATE_LIMIT_RPM=60

# Default OpenAI model used by the proxy
OPENAI_MODEL=gpt-4.1-mini

# Optional OpenAI tuning parameters
OPENAI_REASONING_EFFORT=minimal
OPENAI_TEXT_VERBOSITY=low
